<html>
<head>
    <title>Test env</title>
    <link rel="stylesheet" href="{{ STATIC_URL }}css/main.css" type="text/css">
</head>
<body>
    <a href="/"><h1>Hi!</h1></a>

    {% if not request.user.is_authenticated %}
        <input type="button" value="login" onclick="location.href='/account/login'">
        <input type="button" value="register" onclick="location.href='/register'">
        <input type="button" value="recover_pass" onclick="location.href='/account/recover_pass'">
    {% else %}
        <input type="button" value="logout" onclick="location.href='/account/logout'">
        <input type="button" value="change_pass" onclick="location.href='/account/change_pass'">
        <input type="button" value="profile" onclick="location.href='/account/profile'">
{#        <input type="button" value="list" onclick="location.href='/list'">#}
    {% endif %}

    {% block content %}
    {% endblock %}

    <script type="text/javascript" src="/static/js_libs/require.js"></script>
    <script type="text/javascript" src="/static/js/config.js"></script>
</body>
</html>